{
  "name": "dockit",
  "productName": "DocKit",
  "private": true,
  "version": "0.0.9",
  "main": "dist/src/electron/main.js",
  "description": "A faster, better and more stable NoSQL desktop tools",
  "author": "geekfun <support@geekfun.club>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/geek-fun/dockit.git"
  },
  "scripts": {
    "vite:start": "vite",
    "vite:build": "cross-env APP_ENV=prod vue-tsc --noEmit && vite build",
    "start": "tsc && cross-env APP_ENV=dev DEBUG=dockit ELECTRON_DISABLE_SECURITY_WARNINGS=true concurrently vite \" electron .\" \"tsc -w\"",
    "build": "cross-env APP_ENV=prod npm run vite:build && tsc",
    "package": "cross-env APP_ENV=prod npm run vite:build && tsc && electron-forge package",
    "test": "jest --runInBand --coverage --coverageReporters json-summary text html lcov",
    "test:ci": "jest --runInBand --ci --coverage --coverageReporters json-summary text html lcov",
    "preview": "vite preview",
    "lint:fix": "eslint --fix ./",
    "lint:check": "eslint ./",
    "prepare": "husky install",
    "make": "cross-env APP_ENV=prod npm run build && electron-forge make"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "debug": "^4.3.4",
    "electron-store": "^8.1.0",
    "monaco-editor": "^0.45.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "update-electron-app": "^3.0.0",
    "vue": "^3.4.15",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/publisher-github": "^7.2.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.2.0",
    "electron-devtools-assembler": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^9.0.6",
    "jest": "^29.7.0",
    "naive-ui": "^2.37.3",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vue-tsc": "^1.8.27"
  }
}
